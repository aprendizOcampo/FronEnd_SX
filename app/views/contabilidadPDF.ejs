<!--Plantilla para informes. Se puede modificar para
usarla en otro tipo de informes-->

<%- include('partials/dashhead.ejs'); %>
  <div class="pull-left image">
    <img src="img/100px.png" class="img" alt="User Image">
    <h3 class="pull-right"> SINVEX - Reporte de contabilidad </h3>
  </div>
  <div class="row">
  </div>
  <div class="row">
    <div class="col-md-3">
    </div>
    <div class="col-md-5">
      <div class="panel panel-default">
        <div class="panel-body">
          <h3>Reporte contable de la aplicación</h3>
          <p>Fecha de expedición:</p>
          <p>Hora de expedición:</p>
          <p>Nombre del usuario:</p>
          <div>
            <table id="" class="table table-bordered table-striped">
              <thead>
                <tr>
                  <th>Fecha</th>
                  <th>Tercero</th>
                  <th>Valor</th>
                  <th>Tipo</th>
                  <th>Usuario</th>
                </tr>
              </thead>
              <tbody>
                <% libro.forEach((libro) => { %>
                  <tr>
                    <td> <%= libro.fecha %> </td>
                    <td> <%= libro.tipo_accion %> </td>
                    <td> <%= libro.Valor %> </td>
                  </tr>
                <% }) %>
                </tfoot>
            </table>
          </div>
        </div>
      </div>
    </div>
  </div>